{
  "name": "@monkestation/veyra-lib",
  "version": "1.2.3",
  "license": "zlib",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "./dist/",
    "./src/",
    "./types/",
    "./package.json",
    "./README.md"
  ],

  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "biome check .",
    "format": "biome format --write .",
    "lint:fix": "biome check --apply .",
    "ci:check": "biome check --max-diagnostics 1",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm run build",
    "pretest": "pnpm run build",
    "test": "mocha test.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "biome": "^0.3.3",
    "chai": "^6.0.1",
    "mocha": "^11.7.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0"
  }
}
